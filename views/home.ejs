<%-include('header') %>
    <main class="container border p-md-5 p-2" style="margin-top: 10px;">
        <div class="row g-5 g-md-4">
                <% data.forEach(el => { %>
            <div class="col-6 col-md-3 g-5">
                <div class="row">
                    <div class="card border-dark mb-3" style="max-width: 18rem;">
                        <div class="card-header"><%=el.Vapehascategory.name %></div>
                        <img class="card-img-top" src="<%=el.Vapehascategory.img %>" alt="Card image cap">
                        <div class="card-body text-dark">
                            <h5 class="card-title"><%=el.Vapehascategory.merk %></h5>
                            <p class="card-text"><%=el.Vapehascategory.desc %></p>
                            <a href="/login" class="btn btn-outline-dark" type="button">More Info</a>
                        </div>
                    </div>
                </div>
            </div>
            <% }); %>
        </div>
    </main>


    <%-include('footer') %>